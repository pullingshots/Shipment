		<xsd:complexType>
			<xsd:all>
				<xsd:element name="requests" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="request" type="com:Request" minOccurs="0" maxOccurs="unbounded"/>
