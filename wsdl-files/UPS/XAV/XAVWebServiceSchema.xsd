<xsd:schema elementFormDefault="qualified" targetNamespace="http://www.ups.com/XMLSchema/XOLTWS/xav/v1.0" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:common="http://www.ups.com/XMLSchema/XOLTWS/Common/v1.0" xmlns:xav="http://www.ups.com/XMLSchema/XOLTWS/xav/v1.0">  <xsd:import namespace="http://www.ups.com/XMLSchema/XOLTWS/Common/v1.0" schemaLocation="common.xsd"/>  <xsd:element name="XAVRequest">    <xsd:complexType>      <xsd:sequence>        <xsd:element ref="common:Request"/>        <xsd:element minOccurs="0" name="RegionalRequestIndicator" type="xsd:string"/>        <xsd:element minOccurs="0" name="MaximumCandidateListSize" type="xsd:string"/>        <xsd:element name="AddressKeyFormat" type="xav:AddressKeyFormatType"/>      </xsd:sequence>    </xsd:complexType>  </xsd:element>  <xsd:element name="XAVResponse">    <xsd:complexType>      <xsd:sequence>        <xsd:element ref="common:Response"/>        <xsd:choice minOccurs="0">          <xsd:element minOccurs="0" name="ValidAddressIndicator" type="xsd:string"/>          <xsd:element minOccurs="0" name="AmbiguousAddressIndicator" type="xsd:string"/>          <xsd:element minOccurs="0" name="NoCandidatesIndicator" type="xsd:string"/>        </xsd:choice>        <xsd:element minOccurs="0" name="AddressClassification" type="xav:AddressClassificationType"/>        <xsd:element maxOccurs="unbounded" minOccurs="0" name="Candidate" type="xav:CandidateType"/>      </xsd:sequence>    </xsd:complexType>  </xsd:element>  <xsd:complexType name="AddressKeyFormatType">    <xsd:sequence>      <xsd:element minOccurs="0" name="ConsigneeName" type="xsd:string"/>      <xsd:element minOccurs="0" name="AttentionName" type="xsd:string"/>      <xsd:element maxOccurs="unbounded" minOccurs="0" name="AddressLine" type="xsd:string"/>      <xsd:sequence minOccurs="0">        <xsd:element minOccurs="0" name="PoliticalDivision2" type="xsd:string"/>        <xsd:element minOccurs="0" name="PoliticalDivision1" type="xsd:string"/>        <xsd:element minOccurs="0" name="PostcodePrimaryLow" type="xsd:string"/>        <xsd:element minOccurs="0" name="PostcodeExtendedLow" type="xsd:string"/>      </xsd:sequence>      <xsd:element minOccurs="0" name="Region" type="xsd:string"/>      <xsd:element minOccurs="0" name="Urbanization" type="xsd:string"/>      <xsd:element name="CountryCode" type="xsd:string"/>    </xsd:sequence>  </xsd:complexType>  <xsd:complexType name="CandidateType">    <xsd:sequence>      <xsd:element minOccurs="0" name="AddressClassification" type="xav:AddressClassificationType"/>      <xsd:element name="AddressKeyFormat" type="xav:AddressKeyFormatType"/>    </xsd:sequence>  </xsd:complexType>  <xsd:complexType name="AddressClassificationType">    <xsd:sequence>      <xsd:element name="Code" type="xsd:string"/>      <xsd:element name="Description" type="xsd:string"/>    </xsd:sequence>  </xsd:complexType></xsd:schema>